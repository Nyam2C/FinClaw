{
  "name": "finclaw",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/entry.ts",
    "format": "oxfmt --check .",
    "format:fix": "oxfmt --write .",
    "lint": "oxlint --config oxlintrc.json .",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsgo --noEmit"
  },
  "devDependencies": {
    "@types/node": "^25.2.2",
    "@typescript/native-preview": "7.0.0-dev.20260209.1",
    "oxfmt": "^0.28.0",
    "oxlint": "^1.43.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "pnpm@10.4.1",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
